<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="CDP 共通ライブラリスタイルチェック" location="dev/config/cdp_core_lib_checks.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <local-check-config name="CDP 共通テストスタイルチェック" location="dev/config/cdp_core_test_checks.xml" type="project" description="">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="すべて(src/main)" enabled="true" check-config-name="CDP 共通ライブラリスタイルチェック" local="true">
    <file-match-pattern match-pattern="src/main/java/.+\.java" include-pattern="true"/>
  </fileset>
  <fileset name="すべて(src/test)" enabled="true" check-config-name="CDP 共通テストスタイルチェック" local="true">
    <file-match-pattern match-pattern="src/test/java/.+\.java" include-pattern="true"/>
  </fileset>
</fileset-config>
